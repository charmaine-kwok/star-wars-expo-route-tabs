{
  "expo": {
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "name": "Star-Wars-API",
    "owner": "Charmaine Kwok",
    "icon": "./assets/starwars_icon.png",
    "description": "A Star-Wars-API React Native app built with Expo.",
    "splash": {
      "image": "./assets/pngwing.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "scheme": "myapp",
    "web": {
      "bundler": "metro"
    },

    "slug": "Star-Wars-API",
    "ios": {
      "bundleIdentifier": "com.mycompany.StarWarsBarejs",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/pngwing.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "extra": {
      "eas": {
        "projectId": "2a287f8f-9d3d-422a-a743-51d4f48bcb0e"
      }
    }
  }
}
